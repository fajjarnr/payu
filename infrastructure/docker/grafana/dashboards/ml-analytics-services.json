{
  "dashboard": {
    "title": "PayU ML & Analytics Services",
    "tags": ["payu", "ml", "analytics"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "title": "KYC Service - Status",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "up{job=\"kyc-service\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "title": "Analytics Service - Status",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "up{job=\"analytics-service\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "title": "Total Requests Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=~\"kyc-service|analytics-service\"}[5m])) by (job, method, path)",
            "legendFormat": "{{job}} - {{method}} {{path}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "title": "P95 Response Time",
        "type": "graph",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=~\"kyc-service|analytics-service\"}[5m])) by (le, job, path))",
            "legendFormat": "{{job}} - {{path}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 3, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "title": "Error Rate by Service",
        "type": "graph",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=~\"kyc-service|analytics-service\",status=~\"5..\"}[5m])) by (job) / sum(rate(http_requests_total{job=~\"kyc-service|analytics-service\"}[5m])) by (job)",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.05, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "title": "Memory Usage by Service",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=~\"kyc-service|analytics-service\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "title": "CPU Usage by Service",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(process_cpu_seconds_total{job=~\"kyc-service|analytics-service\"}[5m])) by (job) * 100",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        }
      },
      {
        "title": "Active Python Threads",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "python_gc_objects_collected_total{job=~\"kyc-service|analytics-service\"}",
            "legendFormat": "{{job}} GC Collected"
          },
          {
            "expr": "python_info{job=~\"kyc-service|analytics-service\"}",
            "legendFormat": "{{job}} Info"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "title": "KYC - Processing Time by Type",
        "type": "graph",
        "gridPos": {"x": 0, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(kyc_processing_duration_seconds_bucket{job=\"kyc-service\"}[5m])) by (le, type))",
            "legendFormat": "{{type}} P95"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(kyc_processing_duration_seconds_bucket{job=\"kyc-service\"}[5m])) by (le, type))",
            "legendFormat": "{{type}} P50"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "title": "KYC - Verifications by Type",
        "type": "graph",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(kyc_verifications_total{job=\"kyc-service\"}[5m])) by (type)",
            "legendFormat": "{{type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "title": "KYC - ML Model Inference Time",
        "type": "graph",
        "gridPos": {"x": 0, "y": 36, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(kyc_ml_inference_duration_seconds_bucket{job=\"kyc-service\"}[5m])) by (le, model))",
            "legendFormat": "{{model}} P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "title": "Analytics - Query Execution Time",
        "type": "graph",
        "gridPos": {"x": 12, "y": 36, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(analytics_query_duration_seconds_bucket{job=\"analytics-service\"}[5m])) by (le, query_type))",
            "legendFormat": "{{query_type}} P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "title": "Analytics - ML Model Predictions",
        "type": "graph",
        "gridPos": {"x": 0, "y": 44, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(analytics_ml_predictions_total{job=\"analytics-service\"}[5m])) by (model)",
            "legendFormat": "{{model}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "title": "Kafka Messages Processed",
        "type": "graph",
        "gridPos": {"x": 12, "y": 44, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(kafka_consumer_records_consumed_total{job=~\"kyc-service|analytics-service\"}[5m])) by (job)",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      }
    ]
  }
}
